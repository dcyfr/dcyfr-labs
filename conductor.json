{
  "name": "dcyfr-labs",
  "description": "Full-stack developer portfolio with Next.js 16, TypeScript, Tailwind v4, shadcn/ui, MDX blog, GitHub integration",
  "scripts": {
    "setup": "npm install && npm run build:search && npm run typecheck",
    "run": "npm run dev",
    "test": "npm run test:ci",
    "lint": "npm run lint && npm run typecheck",
    "build": "npm run build:search && next build"
  },
  "workspace": {
    "branch": "dcyfr/config-audit",
    "target_branch": "preview",
    "agent_configs": {
      "default": {
        "model": "claude-opus-4.5",
        "fallback_model": "claude-sonnet-4.5",
        "fallback_trigger": "rate_limit_exceeded",
        "max_turns": 25,
        "estimated_cost_per_1k_tokens": 0.015
      },
      "quick_fixes": {
        "model": "claude-haiku-4.5",
        "max_turns": 10,
        "estimated_cost_per_1k_tokens": 0.0025
      },
      "research": {
        "model": "claude-sonnet-4.5",
        "max_turns": 20,
        "estimated_cost_per_1k_tokens": 0.003
      }
    },
    "cost_tracking": {
      "enabled": true,
      "daily_budget": 10.0,
      "monthly_budget": 200.0,
      "alert_threshold": 0.8,
      "log_file": "~/.claude/usage/ai-usage.jsonl"
    },
    "mcp_usage": {
      "perplexity_monthly_limit": 40,
      "context7_monthly_limit": 40
    }
  }
}
