{
	"servers": {
		"GitHub": {
			"type": "http",
			"url": "https://api.githubcopilot.com/mcp/",
			"_tier": "1-essential",
			"_description": "GitHub API for repos, issues, PRs, code search"
		},
		"Vercel": {
			"url": "https://mcp.vercel.com",
			"type": "http",
			"_tier": "1-essential",
			"_description": "Vercel deployment management and preview URLs"
		},
		"DCYFR Analytics": {
			"type": "stdio",
			"command": "npm",
			"args": [
				"run",
				"mcp:analytics"
			],
			"cwd": "${workspaceFolder}",
			"envFile": "${workspaceFolder}/.env.local",
			"_tier": "1-essential",
			"_description": "Project analytics and metrics access"
		},
		"DCYFR DesignTokens": {
			"type": "stdio",
			"command": "npm",
			"args": [
				"run",
				"mcp:tokens"
			],
			"cwd": "${workspaceFolder}",
			"_tier": "1-essential",
			"_description": "Design token validation and lookup"
		},
		"DCYFR ContentManager": {
			"type": "stdio",
			"command": "npm",
			"args": [
				"run",
				"mcp:content"
			],
			"cwd": "${workspaceFolder}",
			"_tier": "1-essential",
			"_description": "Blog and content management operations"
		},
		"Sentry": {
			"url": "https://mcp.sentry.dev/mcp/dcyfr-labs/dcyfr-labs",
			"type": "http",
			"_tier": "2-on-demand",
			"_description": "Error tracking and performance monitoring"
		},
		"Axiom": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"mcp-remote",
				"https://mcp.axiom.co/mcp"
			],
			"_tier": "2-on-demand",
			"_description": "Log aggregation and analysis"
		},
		"Context7": {
			"command": "npx",
			"args": [
				"-y",
				"@upstash/context7-mcp@latest"
			],
			"type": "stdio",
			"_tier": "2-on-demand",
			"_description": "Enhanced context retrieval from Upstash"
		},
		"Octocode": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"octocode-mcp@latest"
			],
			"_tier": "2-on-demand",
			"_description": "GitHub code research and pattern discovery"
		},
		"Perplexity": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@perplexity-ai/mcp-server"
			],
			"envFile": "${workspaceFolder}/.env.local",
			"_tier": "2-on-demand",
			"_description": "AI-powered web search and research"
		},
		"DeepGraph Next.js": {
			"command": "npx",
			"args": [
				"-y",
				"mcp-code-graph@latest",
				"vercel/next.js"
			],
			"disabled": true,
			"_tier": "3-specialized",
			"_description": "Deep analysis of Next.js codebase patterns",
			"_enable_for": "Next.js architecture research, pattern discovery",
			"_enable_command": "Remove 'disabled: true' to enable",
			"type": "stdio"
		},
		"DeepGraph TypeScript": {
			"command": "npx",
			"args": [
				"-y",
				"mcp-code-graph@latest",
				"microsoft/TypeScript"
			],
			"disabled": true,
			"_tier": "3-specialized",
			"_description": "Deep analysis of TypeScript compiler patterns",
			"_enable_for": "TypeScript internals research, type system exploration",
			"_enable_command": "Remove 'disabled: true' to enable",
			"type": "stdio"
		},
		"Playwright": {
			"command": "npx",
			"args": [
				"-y",
				"@executeautomation/playwright-mcp-server"
			],
			"disabled": true,
			"_tier": "3-specialized",
			"_description": "Playwright browser automation and testing",
			"_enable_for": "E2E test development, browser debugging",
			"_enable_command": "Remove 'disabled: true' to enable",
			"type": "stdio"
		},
		"arXiv": {
			"type": "stdio",
			"command": "uvx",
			"args": [
				"arxiv-mcp-server"
			],
			"disabled": true,
			"_tier": "3-specialized",
			"_description": "Academic paper search and retrieval",
			"_enable_for": "Research papers, academic citations, ML papers",
			"_enable_command": "Remove 'disabled: true' to enable"
		}
	}
}