# GitHub Actions CI/CD Environment Variables
# Loaded via 1Password load-secrets-action@v1
#
# Usage in workflow:
#  - name: Load secrets from 1Password
#    uses: 1password/load-secrets-action@v1
#    with:
#      export-env: true
#    env:
#      OP_SERVICE_ACCOUNT_TOKEN: ${{ secrets.OP_SERVICE_ACCOUNT_TOKEN }}
#      VAR_NAME: op://vault/item/field

# GitHub
GITHUB_TOKEN=op://CI-CD/dcyfr-labs-github-token/credential
GITHUB_WEBHOOK_SECRET=op://Development/dcyfr-labs-github-webhook-secret/password

# Vercel
VERCEL_TOKEN=op://CI-CD/dcyfr-labs-vercel-token/credential
VERCEL_AUTOMATION_BYPASS_SECRET=op://Development/dcyfr-labs-vercel-automation-bypass-secret/password

# Inngest
INNGEST_EVENT_KEY=op://Development/dcyfr-labs-inngest-event-key/credential
INNGEST_SIGNING_KEY=op://Development/dcyfr-labs-inngest-signing-key/credential

# Sentry
SENTRY_AUTH_TOKEN=op://Development/dcyfr-labs-sentry-auth-token/credential

# Upstash Redis
UPSTASH_REDIS_REST_TOKEN=op://Development/dcyfr-labs-upstash-redis-rest-token/credential
UPSTASH_REDIS_REST_TOKEN_PREVIEW=op://Development/dcyfr-labs-upstash-redis-preview-token/credential

# Resend
RESEND_API_KEY=op://Development/dcyfr-labs-resend-api-key/credential

# Cron / API Authentication
CRON_SECRET=op://Development/dcyfr-labs-cron-secret/password
